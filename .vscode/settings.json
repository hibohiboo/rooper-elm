{
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.fontFamily": "UbuntuMonospace",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.eol": "\n",
  "eslint.enable": true,
  "typescript.validate.enable": false,
  "scss.validate": false,
  "stylelint.autoFixOnSave": true,
}
